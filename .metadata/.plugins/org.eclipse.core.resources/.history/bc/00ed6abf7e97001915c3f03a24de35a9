package com.revature.d.objects;

public class Dog extends Animal {

	private String breed;
	private String furLength;
	private String size;

	public Dog() {
		super();
		// TODO Auto-generated constructor stub
	}

	public Dog(String name, boolean isAlive) {
		super(name, isAlive);
		// TODO Auto-generated constructor stub
	}

	public Dog(String breed, String furLength, String size) {
		super();
		this.breed = breed;
		this.furLength = furLength;
		this.size = size;
	}

	public Dog(String name, boolean isAlive, String breed, String furLength, String size) {
		super(name, isAlive);
		this.breed = breed;
		this.furLength = furLength;
		this.size = size;
	}

	public String getBreed() {
		return breed;
	}

	public void setBreed(String breed) {
		this.breed = breed;
	}

	public String getFurLength() {
		return furLength;
	}

	public void setFurLength(String furLength) {
		this.furLength = furLength;
	}

	public String getSize() {
		return size;
	}

	public void setSize(String size) {
		this.size = size;
	}

	@Override
	public int hashCode() {
		final int prime = 31;
		int result = super.hashCode();
		result = prime * result + ((breed == null) ? 0 : breed.hashCode());
		result = prime * result + ((furLength == null) ? 0 : furLength.hashCode());
		result = prime * result + ((size == null) ? 0 : size.hashCode());
		return result;
	}

	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (!super.equals(obj))
			return false;
		if (getClass() != obj.getClass())
			return false;
		Dog other = (Dog) obj;
		if (breed == null) {
			if (other.breed != null)
				return false;
		} else if (!breed.equals(other.breed))
			return false;
		if (furLength == null) {
			if (other.furLength != null)
				return false;
		} else if (!furLength.equals(other.furLength))
			return false;
		if (size == null) {
			if (other.size != null)
				return false;
		} else if (!size.equals(other.size))
			return false;
		return true;
	}

	@Override
	public String toString() {
		int[] nums = new int[5]();
		return "Dog [breed=" + breed + ", furLength=" + furLength + ", size=" + size + ", getName=" + getName()
				+ ", isAlive=" + isAlive() + "]";
	}

}

